<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#FFFFFF"
    tools:context=".SignUp_AggiungiCorsoPreferito">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="343dp"
        android:layout_height="26dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="272dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="305dp"
        android:fontFamily="@font/roboto_medium"
        android:gravity="center"
        android:text="Benvenuto"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="343dp"
        android:layout_height="65dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="442dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="96dp"
        android:fontFamily="@font/roboto"
        android:gravity="center"
        android:text="Aiuta la community di Sapnet aggiungendo informazioni sui corsi e aggiornando quelle meno recenti. Te ne saremo grati!"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="343dp"
        android:layout_height="53dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="332dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="218dp"
        android:fontFamily="@font/roboto"
        android:gravity="center"
        android:text="Resta aggiornato su qualsiasi novità legata ai professori, alle lezioni e agli esami!"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="343dp"
        android:layout_height="60dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="382dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="161dp"
        android:fontFamily="@font/roboto"
        android:gravity="center"
        android:text="Pubblica post per i più svariati fini come la vendita di libri, la condivisione di appunti o per effettuare recensioni su insegnanti/esami!"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="104dp"
        android:background="@color/trasparente"
        android:onClick="Add"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:srcCompat="@drawable/bottone_aggiungi_corso_laurea"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="360dp"
        android:layout_height="22dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="312dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="269dp"
        android:fontFamily="@font/roboto"
        android:gravity="center"
        android:text="Segui i corsi di laurea e le materie che preferisci!"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="fill_parent"
        android:layout_height="117dp"
        android:background="@color/rossoSapienza"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rettangolo" />

</androidx.constraintlayout.widget.ConstraintLayout>